theme_classic() +
scale_color_gradient(low="#111E4F",high="#74D3E6")
ggplot(df,aes(x=date_released,y=imdb_score,color=genre_1)) +
geom_point() +
geom_smooth(method = "loess", se = FALSE, color = "#111E4F") +
theme_classic()
